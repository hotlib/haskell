
var records = #{toJSON s}

$(document).ready(function() {
    $.each(records.questions, function(i,v) {
    	addQuestion(v);
    });
});


var questionInput = function(t) {
	return $("<label>").text(t);
}

var addQuestion = function(text) {
	var el = $("<div>");
	el
	.append( questionInput(text))
	.appendTo( $("#myform"));
}

